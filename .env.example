# Environment Configuration for Ollama Chat
# Copy this file to .env and fill in your values
# DO NOT commit .env to version control

# Auth0 Configuration
AUTH0_DOMAIN=your-tenant.auth0.com
AUTH0_CLIENT_ID=your_client_id_here
AUTH0_AUDIENCE=ollama-chat-api

# AWS Configuration
INSTANCE_ID=i-xxxxxxxxxxxxx
ADMIN_EMAILS=your-admin@email.com

# Cost Protection (optional)
BUDGET_EMAIL=your-email@example.com
MONTHLY_BUDGET_USD=10

# CloudFront URL (leave empty for first deployment, add after)
CLOUDFRONT_URL=

# CDK Configuration
CDK_DEFAULT_ACCOUNT=your-aws-account-id
CDK_DEFAULT_REGION=eu-west-1

# OPTION 1: Set environment variables above
# OPTION 2: Or add values directly to infra/cdk.json "context" section
# See infra/cdk.json.example for structure
